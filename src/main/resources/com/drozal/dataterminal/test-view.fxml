<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<GridPane xmlns:fx="http://javafx.com/fxml/1" vgap="3.0" xmlns="http://javafx.com/javafx/22"
          fx:controller="com.drozal.dataterminal.TestView">
    <columnConstraints>
        <ColumnConstraints halignment="LEFT" hgrow="NEVER" minWidth="10.0" percentWidth="40.0"/>
        <ColumnConstraints halignment="LEFT" hgrow="ALWAYS" minWidth="10.0" percentWidth="60.0"/>
    </columnConstraints>
    <rowConstraints>
        <RowConstraints minHeight="10.0" vgrow="SOMETIMES"/>
        <RowConstraints minHeight="10.0" vgrow="SOMETIMES"/>
    </rowConstraints>
    <children>
        <Label text="{case1.getCaseNum()}">
            <font>
                <Font name="Segoe UI Black" size="12.0"/>
            </font>
        </Label>
        <Label text="{case1.getName()}" GridPane.columnIndex="1">
            <font>
                <Font name="Segoe UI Semibold" size="12.0"/>
            </font>
        </Label>
        <Label text="{case1.getOffenceDate()}" GridPane.rowIndex="1">
            <font>
                <Font name="Segoe UI Semibold" size="12.0"/>
            </font>
        </Label>
        <Label text="{case1.getOffenceTime()}" GridPane.columnIndex="1" GridPane.rowIndex="1">
            <font>
                <Font name="Segoe UI Semibold" size="12.0"/>
            </font>
        </Label>
    </children>
    <padding>
        <Insets bottom="2.0" left="3.0" right="3.0" top="2.0"/>
    </padding>
</GridPane>
