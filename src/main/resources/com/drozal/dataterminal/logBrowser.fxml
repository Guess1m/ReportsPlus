<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.effect.ColorAdjust?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.paint.Color?>
<?import javafx.scene.text.Font?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="603.0" prefWidth="891.0"
            style="-fx-background-color: rgb(0,0,0,0.69);" xmlns="http://javafx.com/javafx/21"
            fx:controller="com.drozal.dataterminal.LogBrowserController">
    <children>
        <AnchorPane fx:id="vbox" prefHeight="470.0" prefWidth="889.0" style="-fx-background-color: #f2f2f2;"
                    AnchorPane.bottomAnchor="1.1" AnchorPane.leftAnchor="1.1" AnchorPane.rightAnchor="1.1"
                    AnchorPane.topAnchor="1.1">
            <children>
                <SplitPane layoutY="50.0" orientation="VERTICAL" prefHeight="550.0" prefWidth="889.0"
                           AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
                           AnchorPane.topAnchor="51.0">
                    <items>
                        <TabPane fx:id="tabPane" prefHeight="384.0" prefWidth="889.0"
                                 stylesheets="@css/main/Logtabpane.css">
                            <tabs>
                                <Tab fx:id="calloutTab" closable="false" text="Callouts">
                                    <content>
                                        <ScrollPane fitToHeight="true" fitToWidth="true" prefHeight="556.0"
                                                    prefWidth="779.0" stylesheets="@css/main/Logscrollpane.css">
                                            <content>
                                                <TableView fx:id="calloutTable" prefHeight="556.0" prefWidth="779.0"
                                                           styleClass="calloutTABLE" stylesheets="@css/tableCss.css"/>
                                            </content>
                                        </ScrollPane>
                                    </content>
                                </Tab>
                                <Tab fx:id="trafficstopTab" closable="false" text="Traffic Stops">
                                    <content>
                                        <ScrollPane fitToHeight="true" fitToWidth="true" prefHeight="556.0"
                                                    prefWidth="779.0" stylesheets="@css/main/Logscrollpane.css">
                                            <content>
                                                <TableView fx:id="trafficStopTable" prefHeight="556.0" prefWidth="779.0"
                                                           styleClass="calloutTABLE" stylesheets="@css/tableCss.css"/>
                                            </content>
                                        </ScrollPane>
                                    </content>
                                </Tab>
                                <Tab fx:id="incidentTab" closable="false" text="Incidents">
                                    <content>
                                        <ScrollPane fitToHeight="true" fitToWidth="true" prefHeight="556.0"
                                                    prefWidth="779.0" stylesheets="@css/main/Logscrollpane.css">
                                            <content>
                                                <TableView fx:id="incidentTable" prefHeight="556.0" prefWidth="779.0"
                                                           styleClass="calloutTABLE" stylesheets="@css/tableCss.css"/>
                                            </content>
                                        </ScrollPane>
                                    </content>
                                </Tab>
                                <Tab fx:id="searchTab" closable="false" text="Searches">
                                    <content>
                                        <ScrollPane fitToHeight="true" fitToWidth="true" prefHeight="556.0"
                                                    prefWidth="779.0" stylesheets="@css/main/Logscrollpane.css">
                                            <content>
                                                <TableView fx:id="searchTable" prefHeight="556.0" prefWidth="779.0"
                                                           styleClass="calloutTABLE" stylesheets="@css/tableCss.css"/>
                                            </content>
                                        </ScrollPane>
                                    </content>
                                </Tab>
                                <Tab fx:id="arrestTab" closable="false" text="Arrests">
                                    <content>
                                        <ScrollPane fitToHeight="true" fitToWidth="true" prefHeight="556.0"
                                                    prefWidth="779.0" stylesheets="@css/main/Logscrollpane.css">
                                            <content>
                                                <TableView fx:id="arrestTable" prefHeight="556.0" prefWidth="779.0"
                                                           styleClass="calloutTABLE" stylesheets="@css/tableCss.css"/>
                                            </content>
                                        </ScrollPane>
                                    </content>
                                </Tab>
                                <Tab fx:id="patrolTab" closable="false" text="Patrols">
                                    <content>
                                        <ScrollPane fitToHeight="true" fitToWidth="true" prefHeight="556.0"
                                                    prefWidth="779.0" stylesheets="@css/main/Logscrollpane.css">
                                            <content>
                                                <TableView fx:id="patrolTable" prefHeight="556.0" prefWidth="779.0"
                                                           styleClass="calloutTABLE" stylesheets="@css/tableCss.css"/>
                                            </content>
                                        </ScrollPane>
                                    </content>
                                </Tab>
                                <Tab fx:id="citationTab" closable="false" text="Citations">
                                    <content>
                                        <ScrollPane fitToHeight="true" fitToWidth="true" prefHeight="556.0"
                                                    prefWidth="779.0" stylesheets="@css/main/Logscrollpane.css">
                                            <content>
                                                <TableView fx:id="citationTable" prefHeight="556.0" prefWidth="779.0"
                                                           styleClass="calloutTABLE" stylesheets="@css/tableCss.css"/>
                                            </content>
                                        </ScrollPane>
                                    </content>
                                </Tab>
                                <Tab fx:id="impoundTab" closable="false" text="Impounds">
                                    <content>
                                        <ScrollPane fitToHeight="true" fitToWidth="true" prefHeight="556.0"
                                                    prefWidth="779.0" stylesheets="@css/main/Logscrollpane.css">
                                            <content>
                                                <TableView fx:id="impoundTable" prefHeight="556.0" prefWidth="779.0"
                                                           styleClass="calloutTABLE" stylesheets="@css/tableCss.css"/>
                                            </content>
                                        </ScrollPane>
                                    </content>
                                </Tab>
                            </tabs>
                            <effect>
                                <DropShadow height="0.0" radius="0.0" width="0.0">
                                    <color>
                                        <Color opacity="0.0"/>
                                    </color>
                                </DropShadow>
                            </effect>
                        </TabPane>
                    </items>
                </SplitPane>
                <AnchorPane layoutX="780.0" layoutY="55.0" prefHeight="37.0" prefWidth="109.0"
                            AnchorPane.leftAnchor="780.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="55.0">
                    <children>
                        <Button fx:id="refreshBtn" graphicTextGap="5.0" layoutX="8.0" layoutY="2.0"
                                mnemonicParsing="false" onAction="#onRefreshButtonClick" opacity="0.95"
                                prefHeight="32.0" prefWidth="99.0" style="-fx-font-size: 11;" styleClass="incidentbtn"
                                stylesheets="@css/menu/menuStyles.css" text="Refresh" textFill="#828797"
                                AnchorPane.rightAnchor="3.0" AnchorPane.topAnchor="2.0">
                            <font>
                                <Font size="18.0"/>
                            </font>
                            <graphic>
                                <ImageView fitHeight="11.0" fitWidth="12.0" pickOnBounds="true" preserveRatio="true">
                                    <image>
                                        <Image url="@imgs/icons/refresh.png"/>
                                    </image>
                                    <effect>
                                        <ColorAdjust brightness="1.0"/>
                                    </effect>
                                </ImageView>
                            </graphic>
                        </Button>
                    </children>
                </AnchorPane>
                <AnchorPane layoutX="36.0" layoutY="-86.0" onMouseDragged="#onMouseDrag" onMousePressed="#onMousePress"
                            prefHeight="51.0" prefWidth="889.0" styleClass="incidentpane"
                            stylesheets="@css/form/formPaneHeader.css" AnchorPane.leftAnchor="0.0"
                            AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                    <children>
                        <Label layoutX="14.0" layoutY="3.0" styleClass="databaseReportLabel"
                               stylesheets="@css/tableCss.css" text="DatabaseReport" textFill="WHITE"
                               AnchorPane.leftAnchor="14.0" AnchorPane.topAnchor="3.0">
                            <font>
                                <Font name="Segoe UI Semibold" size="36.0"/>
                            </font>
                        </Label>
                        <Label layoutX="240.0" layoutY="10.0" styleClass="viewerLabel" stylesheets="@css/tableCss.css"
                               text="Viewer" textFill="#cdcdcd" AnchorPane.leftAnchor="240.0"
                               AnchorPane.topAnchor="10.0">
                            <font>
                                <Font name="Segoe UI Semibold" size="36.0"/>
                            </font>
                        </Label>
                        <ImageView fitHeight="27.0" fitWidth="24.0" layoutX="948.0" layoutY="2.0"
                                   onMouseClicked="#onExitButtonClick" pickOnBounds="true" preserveRatio="true"
                                   AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="10.0">
                            <image>
                                <Image url="@imgs/icons/cross.png"/>
                            </image>
                            <effect>
                                <ColorAdjust brightness="1.0"/>
                            </effect>
                        </ImageView>
                        <ImageView fitHeight="20.0" fitWidth="21.0" layoutX="825.0" layoutY="12.0"
                                   onMouseClicked="#onFullscreenBtnClick" pickOnBounds="true" preserveRatio="true"
                                   AnchorPane.rightAnchor="44.0" AnchorPane.topAnchor="12.0">
                            <image>
                                <Image url="@imgs/icons/maximize.png"/>
                            </image>
                            <effect>
                                <ColorAdjust brightness="1.0"/>
                            </effect>
                        </ImageView>
                    </children>
                </AnchorPane>
            </children>
        </AnchorPane>
    </children>
</AnchorPane>
