<?xml version="1.0" encoding="UTF-8"?><?import javafx.geometry.*?><?import javafx.scene.control.*?><?import javafx.scene.layout.*?>
<BorderPane xmlns:fx="http://javafx.com/fxml/1" fx:id="root" prefHeight="710.0" prefWidth="1003.0" stylesheets="@../../css/callout/calloutManager.css" xmlns="http://javafx.com/javafx/21" fx:controller="com.Guess.ReportsPlus.Windows.Apps.CalloutViewController">
    <center>
        <StackPane fx:id="calloutPane" BorderPane.alignment="CENTER">
            <children>
                <VBox StackPane.alignment="CENTER">
                    <children>
                        <StackPane VBox.vgrow="ALWAYS">
                            <children>
                                <TableView fx:id="activeCalloutsTable" StackPane.alignment="CENTER">
                                    <columnResizePolicy>
                                        <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
                                    </columnResizePolicy>
                                </TableView>
                                <VBox fx:id="historyCalloutsContainer" alignment="TOP_RIGHT">
                                    <children>
                                        <TableView fx:id="historyCalloutsTable" VBox.vgrow="ALWAYS">
                                            <columnResizePolicy>
                                                <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
                                            </columnResizePolicy>
                                        </TableView>
                                    </children>
                                </VBox>
                                <GridPane fx:id="callInfoContainer" hgap="5.0" style="-fx-background-color: rgb(0,0,0,0.09);" vgap="5.0">
                                    <columnConstraints>
                                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0"/>
                                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0"/>
                                        <ColumnConstraints hgrow="SOMETIMES" minWidth="20.0"/>
                                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0"/>
                                    </columnConstraints>
                                    <rowConstraints>
                                        <RowConstraints minHeight="-Infinity" vgrow="NEVER"/>
                                        <RowConstraints minHeight="-Infinity" vgrow="NEVER"/>
                                        <RowConstraints minHeight="-Infinity" vgrow="NEVER"/>
                                        <RowConstraints minHeight="10.0" vgrow="SOMETIMES"/>
                                    </rowConstraints>
                                    <children>
                                        <HBox alignment="CENTER_LEFT" spacing="5.0">
                                            <children>
                                                <Label fx:id="callInfoNumber" style="-fx-font-family: &quot;Inter 28pt Medium&quot;;" text="Call#"/>
                                                <TextField fx:id="callInfoNumberField" editable="false" HBox.hgrow="ALWAYS"/>
                                            </children>
                                            <padding>
                                                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                                            </padding>
                                        </HBox>
                                        <HBox alignment="CENTER_LEFT" spacing="5.0" GridPane.columnIndex="3">
                                            <children>
                                                <Label fx:id="callInfoArea" style="-fx-font-family: &quot;Inter 28pt Medium&quot;;" text="Area"/>
                                                <TextField fx:id="callInfoAreaField" editable="false" HBox.hgrow="ALWAYS"/>
                                            </children>
                                            <padding>
                                                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                                            </padding>
                                        </HBox>
                                        <HBox alignment="CENTER_LEFT" spacing="5.0" GridPane.rowIndex="2">
                                            <children>
                                                <Label fx:id="callInfoDate" style="-fx-font-family: &quot;Inter 28pt Medium&quot;;" text="Date"/>
                                                <TextField fx:id="callInfoDateField" editable="false" HBox.hgrow="ALWAYS"/>
                                            </children>
                                            <padding>
                                                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                                            </padding>
                                        </HBox>
                                        <HBox alignment="CENTER_LEFT" spacing="5.0" GridPane.columnIndex="1" GridPane.columnSpan="2">
                                            <children>
                                                <Label fx:id="callInfoStreet" style="-fx-font-family: &quot;Inter 28pt Medium&quot;;" text="Street"/>
                                                <TextField fx:id="callInfoStreetField" editable="false" HBox.hgrow="ALWAYS"/>
                                            </children>
                                            <padding>
                                                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                                            </padding>
                                        </HBox>
                                        <HBox alignment="CENTER_LEFT" spacing="5.0" GridPane.columnIndex="3" GridPane.rowIndex="1">
                                            <children>
                                                <Label fx:id="callInfoCounty" style="-fx-font-family: &quot;Inter 28pt Medium&quot;;" text="County"/>
                                                <TextField fx:id="callInfoCountyField" editable="false" HBox.hgrow="ALWAYS"/>
                                            </children>
                                            <padding>
                                                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                                            </padding>
                                        </HBox>
                                        <HBox alignment="CENTER_LEFT" spacing="5.0" GridPane.columnIndex="1" GridPane.rowIndex="2">
                                            <children>
                                                <Label fx:id="callInfoTime" style="-fx-font-family: &quot;Inter 28pt Medium&quot;;" text="Time"/>
                                                <TextField fx:id="callInfoTimeField" editable="false" HBox.hgrow="ALWAYS"/>
                                            </children>
                                            <padding>
                                                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                                            </padding>
                                        </HBox>
                                        <HBox alignment="CENTER_LEFT" spacing="5.0" GridPane.rowIndex="1">
                                            <children>
                                                <Label fx:id="callInfoPriority" style="-fx-font-family: &quot;Inter 28pt Medium&quot;;" text="Priority"/>
                                                <TextField fx:id="callInfoPriorityField" editable="false" HBox.hgrow="ALWAYS"/>
                                            </children>
                                            <padding>
                                                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                                            </padding>
                                        </HBox>
                                        <HBox alignment="CENTER_LEFT" spacing="5.0" GridPane.columnIndex="1" GridPane.columnSpan="2" GridPane.rowIndex="1">
                                            <children>
                                                <Label fx:id="callInfoType" style="-fx-font-family: &quot;Inter 28pt Medium&quot;;" text="Type"/>
                                                <TextField fx:id="callInfoTypeField" editable="false" HBox.hgrow="ALWAYS"/>
                                            </children>
                                            <padding>
                                                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                                            </padding>
                                        </HBox>
                                        <Button fx:id="callInfoDeleteButton" mnemonicParsing="false" onAction="#callInfoDeleteBtnPress" styleClass="delete-Button" text="Delete" GridPane.columnIndex="3" GridPane.halignment="CENTER" GridPane.rowIndex="2"/>
                                        <TabPane fx:id="callInfoTabPane" tabClosingPolicy="UNAVAILABLE" GridPane.columnSpan="2147483647" GridPane.rowIndex="3" GridPane.rowSpan="2147483647">
                                            <tabs>
                                                <Tab fx:id="callInfoNotesTab" text="Notes">
                                                    <content>
                                                        <VBox>
                                                            <children>
                                                                <ScrollPane fitToHeight="true" fitToWidth="true" VBox.vgrow="ALWAYS">
                                                                    <content>
                                                                        <BorderPane fx:id="notesContainerParent" style="-fx-background-color: rgb(0,0,0,0.05);">
                                                                            <center>
                                                                                <VBox fx:id="notesContainer">
                                                                                    <BorderPane.margin>
                                                                                        <Insets/>
                                                                                    </BorderPane.margin>
                                                                                    <padding>
                                                                                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                                                                                    </padding>
                                                                                </VBox>
                                                                            </center>
                                                                        </BorderPane>
                                                                    </content>
                                                                </ScrollPane>
                                                                <HBox fx:id="callInfoTypeContainer" alignment="CENTER" spacing="20.0" style="-fx-border-width: 1;" VBox.vgrow="NEVER">
                                                                    <children>
                                                                        <TextField fx:id="callInfoCommentField" promptText="Comment.." style="-fx-padding: 5;" HBox.hgrow="ALWAYS"/>
                                                                        <Button fx:id="callInfoTypeSend" mnemonicParsing="false" onAction="#onSendNoteClick" style="-fx-border-radius: 7;" text="Send"/>
                                                                    </children>
                                                                    <padding>
                                                                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                                                                    </padding>
                                                                </HBox>
                                                            </children>
                                                        </VBox>
                                                    </content>
                                                </Tab>
                                            </tabs>
                                            <GridPane.margin>
                                                <Insets top="10.0"/>
                                            </GridPane.margin>
                                        </TabPane>
                                    </children>
                                    <StackPane.margin>
                                        <Insets/>
                                    </StackPane.margin>
                                    <padding>
                                        <Insets bottom="15.0" left="15.0" right="15.0" top="15.0"/>
                                    </padding>
                                </GridPane>
                            </children>
                        </StackPane>
                        <GridPane maxHeight="-Infinity" minHeight="-Infinity" VBox.vgrow="NEVER">
                            <columnConstraints>
                                <ColumnConstraints hgrow="SOMETIMES" minWidth="40.0"/>
                                <ColumnConstraints hgrow="SOMETIMES" minWidth="40.0"/>
                                <ColumnConstraints hgrow="SOMETIMES" minWidth="40.0"/>
                            </columnConstraints>
                            <rowConstraints>
                                <RowConstraints minHeight="10.0" vgrow="SOMETIMES"/>
                            </rowConstraints>
                            <children>
                                <HBox alignment="CENTER" spacing="10.0" GridPane.halignment="CENTER" GridPane.valignment="CENTER">
                                    <children>
                                        <ToggleButton fx:id="displayActiveCallsBtn" mnemonicParsing="false" style="-fx-background-color: transparent; -fx-border-radius: 7;" text="Active Calls"/>
                                        <ToggleButton fx:id="displayCallHistoryBtn" mnemonicParsing="false" style="-fx-background-color: transparent; -fx-border-radius: 7;" text="Call History"/>
                                    </children>
                                    <padding>
                                        <Insets bottom="15.0" left="15.0" right="15.0" top="15.0"/>
                                    </padding>
                                </HBox>
                                <HBox alignment="CENTER" spacing="10.0" GridPane.columnIndex="2" GridPane.halignment="CENTER" GridPane.valignment="CENTER">
                                    <children>
                                        <Button fx:id="settingsBtn" mnemonicParsing="false" onAction="#settingsBtnPress" style="-fx-background-color: transparent; -fx-border-radius: 7;" text="Settings"/>
                                        <Button fx:id="exitBtn" mnemonicParsing="false" onAction="#exitBtnPress" style="-fx-background-color: transparent; -fx-border-radius: 7;" text="Exit"/>
                                    </children>
                                </HBox>
                                <HBox alignment="CENTER" spacing="10.0" GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.valignment="CENTER">
                                    <children>
                                        <Button fx:id="createCallBtn" mnemonicParsing="false" onAction="#createCallBtnPress" style="-fx-background-color: transparent; -fx-border-radius: 7;" text="Create Call"/>
                                    </children>
                                </HBox>
                            </children>
                        </GridPane>
                    </children>
                </VBox>
            </children>
        </StackPane>
    </center>
</BorderPane>
