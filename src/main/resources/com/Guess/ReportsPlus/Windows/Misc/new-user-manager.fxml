<?xml version="1.0" encoding="UTF-8"?><?import javafx.geometry.Insets?><?import javafx.scene.control.Button?><?import javafx.scene.control.Label?><?import javafx.scene.control.ScrollPane?><?import javafx.scene.layout.*?><?import javafx.scene.text.Font?>
<BorderPane xmlns:fx="http://javafx.com/fxml/1" fx:id="root" prefHeight="553.0" prefWidth="628.0"
            stylesheets="@../../css/userManager/userManager.css" xmlns="http://javafx.com/javafx/21"
            fx:controller="com.Guess.ReportsPlus.Windows.Misc.NewUserManagerController">
    <center>
        <AnchorPane fx:id="mainPane" style="-fx-background-color: transparent;" BorderPane.alignment="CENTER">
            <padding>
                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
            </padding>
            <children>
                <ScrollPane fitToHeight="true" fitToWidth="true" layoutX="5.0" layoutY="5.0"
                            stylesheets="@../../css/main/scrollPane.css" AnchorPane.bottomAnchor="0.0"
                            AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                    <content>
                        <VBox>
                            <padding>
                                <Insets bottom="25.0" left="25.0" right="25.0" top="25.0"/>
                            </padding>
                            <children>
                                <BorderPane>
                                    <left>
                                        <Label fx:id="header" text="Active User Profiles" BorderPane.alignment="CENTER">
                                            <font>
                                                <Font name="Inter 28pt Bold" size="21.0"/>
                                            </font>
                                        </Label>
                                    </left>
                                    <right>
                                        <Button fx:id="newUserBtn" mnemonicParsing="false" onAction="#createUserBtn"
                                                style="-fx-background-color: rgb(0,0,0,0.1); -fx-background-radius: 7; -fx-padding: 5 15;"
                                                text="Create New User" BorderPane.alignment="CENTER">
                                            <font>
                                                <Font name="Inter 28pt Bold" size="12.0"/>
                                            </font>
                                        </Button>
                                    </right>
                                </BorderPane>
                                <VBox fx:id="usersList" spacing="20.0" VBox.vgrow="ALWAYS">
                                    <VBox.margin>
                                        <Insets top="25.0"/>
                                    </VBox.margin>
                                </VBox>
                            </children>
                        </VBox>
                    </content>
                </ScrollPane>
            </children>
        </AnchorPane>
    </center>
</BorderPane>
